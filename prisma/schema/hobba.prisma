enum HobbaGroup {
    GOLD
    SILVER
}

model Hobba {
    id String @id @default(cuid())

    name             String   @unique
    motto            String?
    imageUrl         String
    lastOnline       DateTime
    accountCreatedAt DateTime

    hobbaGroup HobbaGroup

    updatedAt DateTime @updatedAt

    @@map("hobba")
}
